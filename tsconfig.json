{
	"include": ["./Assets/**/*"],
	"exclude": ["node_module", "dist"],
	"compilerOptions": {
		"allowJs": true,
		"rootDir": ".",
		"target": "es2021",
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"isolatedModules": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"skipLibCheck": true,
		"noEmit": true,
		"paths": {}
	}
}
