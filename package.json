{
	"name": "starter-nessa-starterkit",
	"version": "5.0.0",
	"description": "StarterNessa is a modern template theme",
	"main": "app.js",
	"author": "StarterTeam",
	"license": "private",
	"private": true,
	"scripts": {
		"set-node-version": "n 16",
		"start": "npm run dev",
		"dev": "npm run dev:webpack",
		"dev:webpack": "cross-env NODE_ENV=development webpack --watch --config config/webpack.development.js",
		"build": "npm run build:webpack",
		"build:webpack": "cross-env NODE_ENV=production webpack --config config/webpack.production.js",
		"lint": "npm run lint:js && npm run lint:scss",
		"lint:js": "eslint . --fix-dry-run",
		"lint:scss": "stylelint \"./src/**/*.scss\" --config .stylelintrc.js --ignore-path .stylelintignore",
		"lint:fix:js": "eslint . --fix",
		"lint:fix:scss": "stylelint \"./src/**/*.scss\" --fix --config .stylelintrc.js --ignore-path .stylelintignore",
		"test": "echo \"No test specified\" && exit 0"
	},
	"dependencies": {
		"animate.css": "^4.1.1",
		"bootstrap": "^4.5.0",
		"isotope-layout": "^3.0.6",
