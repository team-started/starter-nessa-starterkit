		"lazysizes": "^4.1.0",
		"owl.carousel": "^2.3.4",
		"picturefill": "^3.0.2",
		"poly-fluid-sizing": "^1.1.0",
		"popper.js": "^1.16.1",
		"smartmenus": "^1.1.0",
		"venobox": "^1.9.1"
	},
